{
  "exclude":[
    "node_modules",
    "compiled"
  ],
  "compilerOptions": {
    // "target":"es2020",
    // "module":"commonjs",
    "outDir":"./compiled/esm",
    "declaration": true,
    // "declarationDir": "./compiled/esm",

    "target": "ES2020",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    // "noEmit": true,
    "jsx": "react-jsx"
  }
}
